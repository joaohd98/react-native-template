{
  "name": "RectNativeTemplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "set-env-prod": "ENVFILE=.env",
    "set-env-dev": "ENVFILE=.env.dev",
    "ios-dev": "npm run set-env-dev react-native run-ios",
    "ios-store": "npm run set-env-prod react-native run-ios",
    "android-dev": "npm run set-env-dev react-native run-android",
    "android-store": "npm run set-env-prod react-native run-android",
    "start": "react-native start",
    "test": "jest --silent --detectOpenHandles",
    "lint": "eslint --fix . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@react-native-community/async-storage": "1.8.1",
    "@react-native-community/masked-view": "0.1.7",
    "@react-native-community/netinfo": "5.7.0",
    "@react-navigation/native": "5.1.4",
    "@react-navigation/stack": "5.2.9",
    "axios": "0.19.2",
    "moment": "2.24.0",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-native": "0.62.1",
    "react-native-config": "1.0.0",
    "react-native-gesture-handler": "1.6.1",
    "react-native-linear-gradient": "2.5.6",
    "react-native-reanimated": "1.7.1",
    "react-native-safe-area-context": "0.7.3",
    "react-native-screens": "2.4.0",
    "react-native-svg": "12.1.0",
    "react-redux": "latest",
    "redux": "4.0.5",
    "redux-persist": "6.0.0",
    "redux-thunk": "2.3.0",
    "styled-components": "5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^0.0.7",
    "@types/jest": "^24.0.24",
    "@types/react-native": "^0.61.15",
    "@types/react-redux": "7.1.7",
    "@types/react-test-renderer": "16.9.1",
    "@types/styled-components": "5.0.1",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "babel-jest": "^24.9.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "5.16.0",
    "husky": "4.2.3",
    "jest": "^25.2.4",
    "jsdom": "16.2.2",
    "metro-react-native-babel-preset": "^0.56.0",
    "prettier": "2.0.2",
    "react-devtools": "4.6.0",
    "react-test-renderer": "16.9.0",
    "redux-logger": "3.0.6",
    "ts-jest": "25.3.0",
    "typescript": "^3.7.3"
  },
  "jest": {
    "preset": "react-native",
    "testEnvironment": "jsdom",
    "setupFiles": [
      "./jest-setup-file.js",
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|react-navigation|@react-navigation/.*)) | /node_modules/@react-native-community/async-storage/(?!(lib))"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test"
    }
  }
}
